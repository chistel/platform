{
    "name": "chistel/platform",
    "description": "Core utilities and components for use in all laravel projects.",
    "type": "project",
    "homepage": "https://github.com/chistel/platform",
    "authors": [
        {
            "name": "Daniel Ukwu",
            "email": "chistelbrown@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "africastalking/africastalking": "^3.0",
        "beberlei/assert": "^3.3",
        "erusev/parsedown": "^1.7.1",
        "ezyang/htmlpurifier": "^4.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^v8.80.0",
        "laravel/helpers": "^1.4",
        "laravel/horizon": "^5.7",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.1",
        "monolog/monolog": "^2.0",
        "mpratt/embera": "^2.0",
        "mustache/mustache": "^2.14",
        "pbmedia/laravel-ffmpeg": "^7.5",
        "plank/laravel-metable": "^4.1",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.7",
        "propaganistas/laravel-phone": "^4.3",
        "pusher/pusher-php-server": "^5.0",
        "rlanvin/php-rrule": "^2.2",
        "spatie/emoji": "^2.2",
        "spatie/image": "^1.10",
        "spatie/laravel-mail-preview": "^4.0",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-mix-preload": "^1.2",
        "spatie/laravel-permission": "^3.17",
        "spatie/laravel-referer": "^1.7",
        "spatie/laravel-sluggable": "^2.6",
        "tymon/jwt-auth": "^1.0",
        "yabacon/paystack-php": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Platform\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Platform\\Providers\\PlatformServiceProvider"
            ]
        }
    }
}
